{
    "name": "lassy-xpath",
    "version": "0.3.6",
    "description": "XPath format and editor for searching LASSY XML files",
    "main": "index.js",
    "types": "index.d.ts",
    "scripts": {
        "prepublishOnly": "npm run build && npm run test",
        "build": "npm run build:ngc && npm run build:copyfiles",
        "build:ngc": "ngc",
        "build:copyfiles": "copyfiles -u 2 dist/src/**/**/**/**/* .",
        "clean": "git clean -xdf -e node_modules",
        "test": "karma start karma.conf.js --single-run",
        "test:watch": "karma start karma.conf.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/UUDigitalHumanitieslab/lassy-xpath.git"
    },
    "author": "Sheean Spoel, Digital Humanities Lab <s.j.j.spoel@uu.nl>",
    "license": "CC-BY-NC-SA-4.0",
    "devDependencies": {
        "@angular/common": "^6.1.6",
        "@angular/compiler": "^6.1.6",
        "@angular/compiler-cli": "^6.1.6",
        "@angular/core": "^6.1.6",
        "@types/es6-shim": "^0.31.37",
        "@types/jasmine": "^2.8.6",
        "@types/jquery": "^3.3.1",
        "@types/node": "^10.9.4",
        "ajv": "^6.5.3",
        "copyfiles": "^2.0.0",
        "core-js": "^2.5.7",
        "jasmine": "^3.1.0",
        "jasmine-core": "^3.1.0",
        "jasmine-ts": "^0.2.1",
        "jquery": "^3.3.1",
        "karma": "^3.0.0",
        "karma-chrome-launcher": "^2.2.0",
        "karma-coverage-istanbul-reporter": "^1.4.2",
        "karma-es6-shim": "^1.0.0",
        "karma-jasmine": "^1.1.1",
        "karma-jasmine-html-reporter": "^1.0.0",
        "karma-jquery": "^0.2.3",
        "karma-typescript": "^3.0.13",
        "karma-webpack": "^3.0.0",
        "rxjs": "^6.3.2",
        "ts-loader": "^4.1.0",
        "typescript": "2.9.2",
        "webpack": "^4.17.2",
        "zone.js": "^0.8.26"
    },
    "dependencies": {
        "brace": "^0.11.1",
        "codelyzer": "^4.5.0",
        "ts-xpath": "^0.3.0",
        "tslint": "^5.11.0"
    }
}
